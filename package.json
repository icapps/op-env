{
    "name": "typescript-starter",
    "version": "1.0.0",
    "description": "Starter template to quickly get started with TypeScript 🏎️",
    "scripts": {
        "start": "node dist/get-env.js",
        "get": "nodemon src/get-env.ts",
        "set": "nodemon src/set-env.ts",
        "build": "rimraf dist && tsc --project tsconfig.build.json",
        "lint": "eslint --ignore-path .gitignore",
        "test": "jest",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/benjiJanssens/typescript-starter.git"
    },
    "keywords": [
        "typescript"
    ],
    "author": "Benjamin Janssens <benji.janssens@gmail.com> (https://benjijanssens.com)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/benjiJanssens/typescript-starter/issues",
        "email": "benji.janssens@gmail.com"
    },
    "homepage": "https://github.com/benjiJanssens/typescript-starter#readme",
    "dependencies": {
        "dotenv-safe": "^8.2.0",
        "envalid": "^7.3.1"
    },
    "devDependencies": {
        "@types/dotenv-safe": "^8.1.2",
        "@types/jest": "^29.2.6",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "lint-staged": "^13.1.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.3",
        "rimraf": "^4.1.2",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
